<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mycompany.backend.dao.lotDao">
	<select id="selectByPage" parameterType="pager" resultType="board">
		SELECT rnum, lot_code, itm_code
		FROM (
		  SELECT ROWNUM as rnum, lot_code, itm_code
		  FROM (
		    SELECT lot_code, itm_code
		    FROM tb_lot
		    ORDER BY lot_code DESC
		  )
		  <!-- WHERE rownum &lt;= #{endRowNo} -->
		)
		<!-- WHERE rnum &gt;= #{startRowNo} -->
	</select>
	
	<select id="count" resultType="int">
		SELECT count(*) FROM tb_lot
	</select>
</mapper>


