<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ows.edu.dao.OrderItemDao">
 	<update id="updateOiUnreleaseQuantity" parameterType="int">
 		UPDATE TB_ORD_ITM
 		SET OI_URLS_QTY=#{combineShipping.receiveUnrelease}
 		WHERE OI_NO=#{combineShipping.orderItemNo}
 	</update>
</mapper>


